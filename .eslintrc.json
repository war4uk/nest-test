{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-use-before-define": [
            "error",
            "nofunc"
        ]
    },
    "globals": {
        "Firebase": false
    }
}